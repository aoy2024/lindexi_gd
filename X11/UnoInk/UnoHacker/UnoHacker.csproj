<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>SamplesApp</AssemblyName>
    <UnoSingleProject>true</UnoSingleProject>
    <OutputType>Library</OutputType>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <DefineConstants>$(DefineConstants);HAS_UNO</DefineConstants>

    <!--
      UnoFeatures let's you quickly add and manage implicit package references based on the features you want to use.
      https://aka.platform.uno/singleproject-features
    -->
    <!--
    <UnoFeatures></UnoFeatures>
    -->
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Uno.UI.Composition">
      <HintPath>C:\lindexi\Library\Uno.UI.Composition.dll</HintPath>
    </Reference>
    <Reference Include="Uno.UI.Runtime.Skia">
      <HintPath>C:\lindexi\Library\Uno.UI.Runtime.Skia.dll</HintPath>
    </Reference>
    <Reference Include="Uno.UI">
      <HintPath>C:\lindexi\Library\Uno.UI.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>
